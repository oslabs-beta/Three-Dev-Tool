{
  "name": "Three Dev Tool",
  "description": "Debugging 3d renders for three.js",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    // "activeTab",
    "webNavigation",
    "scripting"
 ],
 "host_permissions": ["<all_urls>"],
 "content_scripts": [
  {
    "matches": ["http://127.0.0.1:5500/index.html"],
    "js": ["./canvasSpy.js"],
    "run_at": "document_end"
  }
],
 "background": {
  "service_worker": "./background.js"
},
  "minimum_chrome_version": "10.0",
  "devtools_page": "./devtools.html"
}